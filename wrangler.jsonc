{
   "$schema": "node_modules/wrangler/config-schema.json",
   "name": "svelte-cf",
   "main": ".svelte-kit/cloudflare/_worker.js",
   "compatibility_date": "2025-11-25",
   "compatibility_flags": ["nodejs_compat"],
   "assets": {
      "binding": "ASSETS",
      "directory": ".svelte-kit/cloudflare"
   },
   "observability": {
      "enabled": true
   },
   "d1_databases": [
      {
         "binding": "DB"
      }
   ]
}
